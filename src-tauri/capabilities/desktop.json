{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main",
    "splashscreen"
  ],
  "permissions": [
    "core:app:default",
    "core:event:default",
    "core:menu:default",
    "core:path:default",
    "core:resources:default",
    "core:tray:default",
    "core:window:default",
    "core:window:allow-set-title",
    "log:default",
    "shell:allow-open",
    "store:default",
    "window-state:default"
  ]
}

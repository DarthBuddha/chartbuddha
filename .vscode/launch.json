  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug:Tauri",
      "type": "cppvsdbg",
      "request": "launch",
      // change the exe name to your actual exe name
      // (to debug release builds, change `target/debug` to `release/debug`)
      "program": "${workspaceRoot}/src-tauri/target/debug/chartbuddha.exe",
      "cwd": "${workspaceRoot}",
      "preLaunchTask": "ui:debug"
    },
    {
      "name": "Build:App",
      "type": "cppvsdbg",
      "request": "launch",
      // change the exe name to your actual exe name
      // (to debug release builds, change `target/debug` to `release/debug`)
      "program": "${workspaceRoot}/src-tauri/target/debug/chartbuddha.exe",
      "cwd": "${workspaceRoot}",
      "preLaunchTask": "ui:build"
    }
  ]
}
